{
  "name": "flooow",
  "version": "1.0.1",
  "description": "Simple yet powerful video scrubbing library for smooth frame-by-frame playback control!",
  "license": "MIT",
  "author": "LÃ©o Joly",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeoJoly/flooow.git"
  },
  "bugs": {
    "url": "https://github.com/LeoJoly/flooow/issues"
  },
  "homepage": "https://github.com/LeoJoly/flooow#readme",
  "keywords": [
    "video",
    "scrubbing",
    "smooth",
    "frame-by-frame",
    "playback",
    "control",
    "typescript",
    "javascript"
  ],
  "type": "module",
  "main": "./dist/index.iife.js",
  "module": "./dist/flooow.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/flooow.js",
      "require": "./dist/index.iife.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite --host --port 3000",
    "dev:html": "vite --host --open /demo/vanilla/index.html --port 3000",
    "build": "rimraf --glob build/**/* && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts && copyfiles ./package.json build",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts, --fix",
    "lint-staged": "npm run typecheck && npm run lint:fix",
    "format:scripts": "prettier . --write",
    "typecheck": "tsc --noEmit --incremental",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "mp4box": "^2.1.0",
    "ua-parser-js": "^2.0.4"
  }
}
